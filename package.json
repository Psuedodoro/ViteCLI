{
  "name": "@psuedodoro/vitecli",
  "version": "1.0.0",
  "description": "An opinionated NG cli alternative for making apps with ViteJS - containing the file structure influenced by Angular/NG-CLI, but improved for ViteJS and ReactJS as a whole by me (Psuedodoro)",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "package": "pkg . --targets node16-win-x64 --output hello",
    "test": "jest"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  },
  "bin": {
    "vcli": "./build/cli.js"
  },
  "keywords": [
    "vite",
    "react",
    "vitejs",
    "typescript",
    "ng",
    "cli",
    "vitets",
    "reactts",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/shelljs": "^0.8.9",
    "@types/yargs": "^17.0.3",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "jest": "^27.2.5",
    "pkg": "^5.3.3",
    "shelljs": "^0.8.4",
    "ts": "^0.2.2",
    "ts-jest": "^27.0.5",
    "tsc": "^2.0.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "inquirer": "^8.2.0",
    "yargs": "^17.2.1"
  }
